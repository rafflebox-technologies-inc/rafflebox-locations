{
  "name": "@rafflebox-technologies-inc/rafflebox-locations",
  "version": "2.0.1",
  "main": "index",
  "types": "index.d.ts",
  "description": "Library for validating Canadian Postal Codes and US Zip Codes",
  "repository": "git@github.com:rafflebox-technologies-inc/rafflebox-locations.git",
  "author": "Rafflebox Technologies Inc",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "test": "jest --config=jest.config.js",
    "test:ci": "yarn test --ci",
    "build": "tsc",
    "clean": "rm -rf build/",
    "lint": "eslint \"**/*.{ts,js}\"",
    "format": "prettier --write \"**/*.{ts,js,json,md,yml}\"",
    "format:check": "prettier --debug-check \"**/*.{ts,js,json,md}\"",
    "pre-publish": "cp package.json build && cp README.md build"
  },
  "devDependencies": {
    "@rafflebox-technologies-inc/eslint-config-rafflebox": "~0.4.0",
    "@tsconfig/node20": "~20.1.2",
    "@types/jest": "~29.5.12",
    "@types/node": "~20.11.19",
    "eslint": "~8.56.0",
    "husky": "~8.0.1",
    "jest": "~29.7.0",
    "lint-staged": "~12.4.1",
    "prettier": "~3.2.5",
    "ts-jest": "~29.1.2",
    "ts-node": "~10.9.2",
    "typescript": "~5.3.3"
  }
}
