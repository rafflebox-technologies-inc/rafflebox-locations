    "eslint": "^7.0.0",
    "eslint-config-rafflebox": "~0.2.0",
    "husky": "~8.0.1",
    "jest": "~28.1.0",
    "lint-staged": "~12.4.1",
    "prettier": "^2.6.2",
    "ts-jest": "~28.0.2",
    "ts-node": "~10.7.0",
    "typescript": "~4.6.4"
  }
